#include <stdio.h>
#include <string.h>

void firstNonRepeating(char* s, char* result) {
    int freq[26] = {0};
    int q[100005]; // queue of indices
    int front = 0, rear = 0;

    int n = strlen(s);
    for (int i = 0; i < n; i++) {
        int idx = s[i] - 'a';
        freq[idx]++;
        q[rear++] = i; // enqueue index

        while (front < rear && freq[s[q[front]] - 'a'] > 1) {
            front++; // dequeue
        }

        if (front < rear) {
            result[i] = s[q[front]];
        } else {
            result[i] = '#';
        }
    }
    result[n] = '\0';
}
