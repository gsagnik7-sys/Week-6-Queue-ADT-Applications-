from collections import deque, defaultdict

class Solution:
    def firstNonRepeating(self, s):
        freq = defaultdict(int)
        q = deque()
        result = []

        for ch in s:
            freq[ch] += 1
            q.append(ch)

            while q and freq[q[0]] > 1:
                q.popleft()

            if q:
                result.append(q[0])
            else:
                result.append('#')

        return ''.join(result)
